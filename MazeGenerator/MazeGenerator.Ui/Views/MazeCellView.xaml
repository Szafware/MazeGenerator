<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MazeGenerator.Ui.Views.MazeCellView"
             Background="Black">
    <Grid RowDefinitions="*,8*,*" ColumnDefinitions="*,8*,*">
        
        <Rectangle Grid.Row="0" Grid.Column="0" Background="Black"/>
        <Rectangle Grid.Row="1" Grid.Column="0" Background="Black"/>
        <Rectangle Grid.Row="2" Grid.Column="0" Background="Black"/>
        
        <Rectangle Grid.Row="0" Grid.Column="0" Background="Black"/>
        <Rectangle Grid.Row="0" Grid.Column="1" Background="Black"/>
        <Rectangle Grid.Row="0" Grid.Column="2" Background="Black"/>

        <Rectangle Grid.Row="0" Grid.Column="2" Background="Black"/>
        <Rectangle Grid.Row="1" Grid.Column="2" Background="Black"/>
        <Rectangle Grid.Row="2" Grid.Column="2" Background="Black"/>

        <Rectangle Grid.Row="2" Grid.Column="0" Background="Black"/>
        <Rectangle Grid.Row="2" Grid.Column="1" Background="Black"/>
        <Rectangle Grid.Row="2" Grid.Column="2" Background="Black"/>
    </Grid>
</ContentView>
